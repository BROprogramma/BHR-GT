<xs:schema xmlns:dsbhrgt="http://www.broservices.nl/xsd/dsbhr-gt/2.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:brocom="http://www.broservices.nl/xsd/brocommon/3.0"
           xmlns:bhrgtcom="http://www.broservices.nl/xsd/bhrgtcommon/2.0"
           xmlns:gml="http://www.opengis.net/gml/3.2"
           xmlns:gmlsf="http://www.opengis.net/gmlsf/2.0"
           targetNamespace="http://www.broservices.nl/xsd/dsbhr-gt/2.0"
           elementFormDefault="qualified"
           version="1.99.1">

    <xs:annotation>
        <xs:appinfo source="http://schemas.opengis.net/gmlsfProfile/2.0/gmlsfLevels.xsd">
            <gmlsf:ComplianceLevel>0</gmlsf:ComplianceLevel>
        </xs:appinfo>
    </xs:annotation>

    <!--
    <xs:import namespace="http://www.broservices.nl/xsd/brocommon/3.0" schemaLocation="https://schema.broservices.nl/xsd/brocommon/3.0/brocommon.xsd"/>
    <xs:import namespace="http://www.broservices.nl/xsd/bhrgtcommon/2.0" schemaLocation="https://schema.broservices.nl/xsd/bhrgtcommon/2.0/bhrgtcommon.xsd"/>
    <xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="https://schema.broservices.nl/profile/gml/1.0/gml-profile.xsd"/>
    -->
    <xs:import namespace="http://www.broservices.nl/xsd/brocommon/3.0" schemaLocation="brocommon.xsd"/>
    <xs:import namespace="http://www.broservices.nl/xsd/bhrgtcommon/2.0" schemaLocation="bhrgtcommon.xsd"/>
    <xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="gml-profile.xsd"/>

    <xs:element name="dispatchDataRequest" type="brocom:DispatchDataRequestType" />
    <xs:element name="parseFault" type="brocom:ParseFaultType" />

    <xs:element name="dispatchCharacteristicsRequest" type="dsbhrgt:DispatchCharacteristicsRequestType">
        <xs:annotation>
            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het bericht dispatchCharacteristicsRequest is een specialisatie van AbstractDispatchCharacteristicsRequest in de package brocommon, waaraan het een registratieobject afhankelijk element criteria toevoegt.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="DispatchCharacteristicsRequestType">
        <xs:complexContent>
            <xs:extension base="brocom:AbstractDispatchCharacteristicsRequestType">
                <xs:sequence>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="criteria"
                                nillable="false"
                                type="dsbhrgt:BHR_GT_CriteriaSetType">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De afzonderlijke kenmerken waaraan de registratieobjecten moeten voldoen.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="dispatchCharacteristicsResponse" type="dsbhrgt:DispatchCharacteristicsResponseType" />
    <xs:complexType name="DispatchCharacteristicsResponseType">
        <xs:complexContent>
            <xs:extension base="brocom:DispatchResponseType">
                <xs:sequence>
                    <xs:element name="numberOfDocuments"
                                type="xs:integer"
                                minOccurs="0"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Aantal documenten</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0"
                                maxOccurs="unbounded"
                                name="dispatchDocument"
                                nillable="false"
                                type="brocom:AbstractRegistrationObjectPropertyType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="dispatchDataResponse" type="dsbhrgt:DispatchDataResponseType" />
    <xs:complexType name="DispatchDataResponseType">
        <xs:complexContent>
            <xs:extension base="brocom:DispatchResponseType">
                <xs:sequence>
                    <xs:element minOccurs="0"
                                maxOccurs="1"
                                name="dispatchDocument"
                                nillable="false"
                                type="brocom:AbstractRegistrationObjectPropertyType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="BHR_GT_C"
                type="dsbhrgt:BHR_GT_CType"
                substitutionGroup="brocom:AbstractCharacteristics">
        <xs:annotation>
            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De kengegevens van een geotechnisch booronderzoek, dat niet uit registratie is genomen.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="BHR_GT_CType">
        <xs:complexContent>
            <xs:extension base="brocom:AbstractCharacteristicsType">
                <xs:sequence>
                    <xs:element name="surveyPurpose"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:SurveyPurposeType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>SurveyPurpose</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het doel waarvoor het onderzoek is uitgevoerd.
                                Explanation: Onderzoek wordt normaliter projectmatig uitgevoerd, zelfs als het direct gebonden is aan een publieke taak. Het gegeven beschrijft het hogere doel van het project waarvoor het onderzoek is uitgevoerd of preciseert de taak.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="discipline"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:DisciplineType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>Discipline</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De discipline waarbinnen het booronderzoek is uitgevoerd.
                                Explanation: Het vakgebied bepaalt hoe het onderzoek is uitgevoerd en welke gegevens en categorieÃ«n van gegevens vastgelegd kunnen zijn. De gegevensdefinitie dekt het vakgebied geotechniek volledig, maar dat betekent niet dat alle geotechnisch booronderzoek er volledig mee beschreven kan worden. In de uitvoeringspraktijk komt het namelijk voor dat een geotechnisch booronderzoek de grenzen van het vakgebied enigszins overschrijdt en er naast geotechnische gegevens gegevens worden vastgelegd die eigenlijk in het domein van de milieukunde of de archeologie thuishoren. Omdat die disciplines buiten het bereik van de basisregistratie ondergrond liggen, zijn die extra gegevens niet gedefinieerd. Geotechnisch booronderzoek met een dergelijk multidisciplinair karakter wordt in de basisregistratie ondergrond opgenomen maar zonder die extra gegevens. Om gebruikers duidelijk te maken dat de onderzoeksresultaten niet volledig geregistreerd zijn wordt als waarde voor het vakgebied niet de waarde geotechniek vermeld, maar een waarde als geotechniekArcheologie.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="researchReportDate"
                                type="brocom:PartialDateType"
                                minOccurs="0"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De datum waarop de uitvoerder van het booronderzoek alle gegevens van het booronderzoek heeft vastgelegd en het resultaat aan de opdrachtgever kan worden aangeboden, dan wel de feitelijk datum van rapportage.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="localVerticalReferencePoint"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:LocalVerticalReferencePointType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>LocalVerticalReferencePoint</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het punt dat in het booronderzoek is gebruikt als nulpunt voor de diepte.
                                Explanation: Het domein bevat begrippen die naar een oppervlak verwijzen. Het lokaal verticaal referentiepunt geldt als het punt waar het onderzoek aan de ondergrond begonnen is. De laag slib die plaatselijk op de waterbodem ligt, wordt niet tot de ondergrond gerekend en ligt boven het lokaal verticaal referentiepunt.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="offset" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De verticale positie van het lokaal verticaal referentiepunt t.o.v. het verticaal referentievlak.
                                Explanation: De waarde kan positief of negatief zijn. Als de waarde positief is, ligt het lokaal verticaal referentiepunt boven het verticaal referentievlak. Met behulp van de verschuiving kan een diepte omgerekend worden naar een positie ten opzichte van het verticaal referentievlak.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:simpleContent>
                                <xs:restriction base="gml:MeasureType">
                                    <xs:attribute name="uom" type="gml:UomIdentifier" use="required" fixed="m"/>
                                </xs:restriction>
                            </xs:simpleContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="verticalDatum"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:VerticalDatumType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>VerticalDatum</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het referentieniveau voor de verticale positie van het lokaal verticaal referentiepunt.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="boringStartDate"
                                type="brocom:PartialDateType"
                                minOccurs="1"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De datum waarop het boren is begonnen.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="boringEndDate"
                                type="brocom:PartialDateType"
                                minOccurs="1"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De datum waarop het boren is beÃ«indigd.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="rockReached"
                                type="brocom:IndicationYesNoType"
                                minOccurs="1"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De aanduiding die aangeeft of het gesteente is doorboord en is bemonsterd.
                                Explanation: Wanneer de ondergrond niet uit grond maar uit gesteente bestaat, worden in het onderzoek andere gegevens vastgelegd dan gewoonlijk het geval is.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="finalDepthBoring" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De diepte waarop het boren is geÃ«indigd.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:simpleContent>
                                <xs:restriction base="gml:MeasureType">
                                    <xs:attribute name="uom" type="gml:UomIdentifier" use="required" fixed="m"/>
                                </xs:restriction>
                            </xs:simpleContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="descriptionReportDate"
                                type="brocom:PartialDateType"
                                minOccurs="0"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De datum waarop het beschrijven is voltooid en de resultaten zijn vastgelegd.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="descriptionProcedure"
                                minOccurs="0"
                                maxOccurs="1"
                                type="bhrgtcom:DescriptionProcedureType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>DescriptionProcedure</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De procedure die aangeeft onder welke afspraken de monsters zijn beschreven.
                                Explanation: De procedure beschrijft volgens welk stelsel van afspraken de monsters beschreven zijn en welke aspecten worden beschreven. Procedures zijn in het algemeen vastgelegd in een norm, protocol of richtlijn. Dat kan overigens een richtlijn zijn die de uitvoerder voor zichzelf gebruikt. Het gegeven is opgenomen omdat het inzicht biedt in de kwaliteit van het werk. Het gebruik van procedures varieert van vakgebied tot vakgebied en voor grond gelden andere afspraken dan voor gesteente. Voor booronderzoek dat onder kwaliteitsregime IMBRO valt, is de boormonsterbeschrijving altijd onder NEN-EN-ISO 14688-1 tot stand gekomen. Een boormonsterbeschrijving die onder NEN 5104 tot stand is gekomen, valt per definitie onder booronderzoek met kwaliteitsregime IMBRO/A.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="descriptionQuality"
                                minOccurs="0"
                                maxOccurs="3"
                                type="bhrgtcom:DescriptionQualityType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>DescriptionQuality</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De aanduiding voor de mate van detail waarmee de opbouw van de ondergrond in het boorprofiel is beschreven.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="analysisReportDate"
                                type="xs:date"
                                minOccurs="0"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De datum waarop de boormonsteranalyse is voltooid en de resultaten zijn vastgelegd.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="samplingQualityOverview"
                                nillable="false"
                                type="dsbhrgt:SamplingQualityOverviewType">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De lijst met de monsterkwaliteiten die met het bemonsteren zijn beoogd.
                                Explanation: Een bemonsteringskwaliteit wordt maar een keer vermeld.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0"
                                maxOccurs="1"
                                name="determinedPropertiesOverview"
                                nillable="false"
                                type="dsbhrgt:DeterminedPropertiesOverviewType">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De lijst met namen van eigenschappen die tijdens de boormonsteranalyse zijn bepaald.
                                Explanation: Een eigenschap wordt maar een keer vermeld.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="BHR_GT_CPropertyType">
        <xs:sequence minOccurs="1">
            <xs:element ref="dsbhrgt:BHR_GT_C"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="BHR_GT_O"
                type="dsbhrgt:BHR_GT_OType"
                substitutionGroup="brocom:RegistrationObject">
        <xs:annotation>
            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het uitgiftedocument dat de gegevens van een geotechnisch booronderzoek dat niet uit registratie is genomen bevat die voor een standaard afnemer beschikbaar zijn.
                Explanation: Deze versie van de uitgiftedocumenten bevat de boormonsterbeschrijving en een deel van de boormonsteranalyse en dat zijn de bepaling verticale vervorming, maximale ongedraineerde schuifsterkte, korrelgrootteverdeling, watergehalte, organischestofgehalte, kalkgehalte, volumieke massa en volumieke massa vaste delen.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="BHR_GT_OType">
        <xs:complexContent>
            <xs:extension base="brocom:RegistrationObjectType">
                <xs:sequence>
                    <xs:element name="deliveryContext"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:DeliveryContextType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>DeliveryContext</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De rechtsgrond op basis waarvan, of bij afwezigheid daarvan, de activiteit naar aanleiding waarvan, het betreffende gegeven is aangeleverd aan de basisregistratie ondergrond.
                                Explanation: De wetgever stipuleert dat het gegeven moet zijn vastgelegd om inzicht te geven in de relatie met de taken van een bestuursorgaan. Het gegeven geeft inzicht in de maatschappelijke betekenis van de informatie.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="surveyPurpose"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:SurveyPurposeType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>SurveyPurpose</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het doel waarvoor het onderzoek is uitgevoerd.
                                Explanation: Onderzoek wordt normaliter projectmatig uitgevoerd, zelfs als het direct gebonden is aan een publieke taak. Het gegeven beschrijft het hogere doel van het project waarvoor het onderzoek is uitgevoerd of preciseert de taak.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="discipline"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:DisciplineType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>Discipline</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De discipline waarbinnen het booronderzoek is uitgevoerd.
                                Explanation: Het vakgebied is een gegeven dat dient om een categorie van booronderzoek in de registratie ondergrond te identificeren. Het gegeven bepaalt hoe het onderzoek is uitgevoerd en welke gegevens en categorieÃ«n van gegevens vastgelegd kunnen zijn. Naast geotechnisch booronderzoek worden geologisch booronderzoek, toegepast geologisch booronderzoek, bodemkundig booronderzoek en cultuurtechnisch booronderzoek onderscheiden. In de uitvoeringspraktijk komt het voor dat in een booronderzoek gegevens worden vastgelegd die onder een van de vijf vakgebieden vallen en gegevens die eigenlijk in het domein van de milieukunde of de archeologie thuishoren. Omdat die disciplines buiten het bereik van de basisregistratie ondergrond liggen, zijn die extra gegevens niet gedefinieerd. Geotechnisch booronderzoek met een dergelijk multidisciplinair karakter wordt in de basisregistratie ondergrond opgenomen maar zonder die extra gegevens. Om gebruikers duidelijk te maken dat de onderzoeksresultaten niet volledig geregistreerd zijn wordt als waarde voor het vakgebied niet de waarde geotechniek vermeld, maar een waarde als geotechniekArcheologie.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="surveyProcedure"
                                minOccurs="1"
                                maxOccurs="1"
                                type="bhrgtcom:SurveyProcedureType">
                        <xs:annotation>
                            <xs:appinfo>
                                <CodeListName>SurveyProcedure</CodeListName>
                                <CodeListURI/>
                            </xs:appinfo>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De procedure die de uitvoering van projecten waarbinnen het booronderzoek wordt uitgevoerd reguleert en daarmee de kaders bepaalt voor de uitvoering van het booronderzoek.
                                Explanation: De procedure beschrijft de manier van werken, de passende hulpmiddelen en de eisen waaraan de uitvoering moet voldoen. Procedures zijn in het algemeen vastgelegd in een norm, protocol of richtlijn. Dat kan overigens een richtlijn zijn die de uitvoerder voor zichzelf gebruikt. Het gegeven is opgenomen omdat het inzicht biedt in de kwaliteit van het werk.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="researchReportDate"
                                type="brocom:PartialDateType"
                                minOccurs="0"
                                maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De datum waarop de uitvoerder van het booronderzoek alle gegevens van het booronderzoek aan de bronhouder heeft overgedragen of in het geval van historische gegevens de datum waarop alle gegevens zijn vastgesteld.
                                Explanation: Het gegeven is alleen aanwezig wanneer alle deelonderzoeken zijn gerapporteerd en het onderzoek is afgesloten.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="researchOperator"
                                minOccurs="0"
                                maxOccurs="1"
                                type="brocom:OrganizationType">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: Het KvK-nummer van de onderneming of de maatschappelijke activiteit van de rechtspersoon die voor de bronhouder geldt als verantwoordelijk voor de uitvoering van het booronderzoek, of het equivalent daarvan in een handelsregister van een andere lidstaat van de Europese Unie dan Nederland.
                                Explanation: Het gegeven wordt alleen uitgeleverd aan de dataleverancier en de bronhouder.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="siteCharacteristicDetermined"
                                type="brocom:IndicationYesNoType"
                                minOccurs="0"
                                maxOccurs="1"
                                nillable="true">
                        <xs:annotation>
                            <xs:documentation source="http://www.imvertor.org/model-info/technical-documentation">Definition: De aanduiding die aangeeft of in het onderzoek gegevens over de toestand van het terrein zijn vastgelegd die van betekenis zijn voor de beoordeling van de resultaten.
                                Explanation: Voor gegevens die zijn aangeleverd in het kader van archiefoverdracht is zelden informatie over de toestand van het terrein vastgelegd.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="registrationHistory"
                                nillable="false"
                                type="brocom:RegistrationHistoryType"/>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="reportHistory"
                                nillable="false"
                                type="dsbhrgt:ReportHistoryType"/>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="deliveredLocation"
                                nillable="false"
                                type="bhrgtcom:DeliveredLocationType"/>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="deliveredVerticalPosition"
                                nillable="false"
                                type="bhrgtcom:DeliveredVerticalPositionType"/>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="standardizedLocation"
                                nillable="false"
                                type="brocom:StandardizedLocationType"/>
                    <xs:element minOccurs="1"
                                maxOccurs="1"
                                name="boring"
                                nillable="false"
                                type="bhrgtcom:BoringType"/>
                    <xs:element minOccurs="0"
                                maxOccurs="1"
                                name="siteCharacteristic"
                                nillable="false"
                                type="bhrgtcom:SiteCharacteristicType"/>
                    <xs:element minOccurs="0"
