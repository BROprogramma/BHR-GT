                     <xs:attribute name="uom"
                                   type="gml:UomIdentifier"
                                   use="required"
                                   fixed="g/cm3"/>
                  </xs:restriction>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
